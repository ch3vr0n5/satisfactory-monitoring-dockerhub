          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\n  COALESCE(prod.name, cons.name) as name,\n  COALESCE(prod.rate, 0) - COALESCE(cons.rate, 0) as rate\nfrom\n  (\n    SELECT\n      t.name,\n      sum(t.val) as rate\n    from\n      (\n        SELECT\n          jsonb_path_query(data, '$.ingredients[*].Name') :: TEXT as name,\n          (\n            jsonb_path_query(data, '$.ingredients[*].CurrentConsumed') :: TEXT :: NUMERIC\n          ) val\n        FROM\n          cache\n          cross join jsonb_path_query(frm_data, '$[*]') as data\n        where\n          metric = 'factory'\n          AND (data -> 'location' ->> 'x') :: NUMERIC BETWEEN $xmin\n          and $xmax\n          AND (data -> 'location' ->> 'y') :: NUMERIC BETWEEN $ymin\n          and $ymax\n      ) as t\n    group by\n      t.name\n    order by\n      t.name\n  ) as cons full\n  outer join (\n    SELECT\n      t.name,\n      sum(t.val) as rate\n    from\n      (\n        SELECT\n          jsonb_path_query(data, '$.production[*].Name') :: TEXT as name,\n          jsonb_path_query(data, '$.production[*].CurrentProd') :: TEXT :: NUMERIC val\n        FROM\n          cache\n          cross join jsonb_path_query(frm_data, '$[*]') as data\n        where\n          metric = 'factory'\n          AND (data -> 'location' ->> 'x') :: NUMERIC BETWEEN $xmin\n          and $xmax\n          AND (data -> 'location' ->> 'y') :: NUMERIC BETWEEN $ymin\n          and $ymax\n      ) as t\n    group by\n      t.name\n    order by\n      t.name\n  ) as prod on cons.name = prod.name\nwhere\n  COALESCE(prod.rate, 0) - COALESCE(cons.rate, 0) != 0\norder by\n  rate desc,\n  name",
          "refId": "A",
