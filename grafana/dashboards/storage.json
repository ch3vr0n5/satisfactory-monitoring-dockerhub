          "rawQuery": true,
          "rawSql": "select t.building,\nt.inventory,\nt.x,\nt.y,\nt.z,\nt.longitude,\nt.latitude\nfrom (\nselect \ndata ->> 'building' as building,\ndata -> 'inventory' as inventory,\ntrim('\"' from (item -> 'Name')::TEXT) as item,\n(data -> 'location' ->> 'x')::NUMERIC/100 as x,\n(data -> 'location' ->> 'y')::NUMERIC/100 as y,\n(data -> 'location' ->> 'z')::NUMERIC/100 as z,\n(((data -> 'location' ->> 'x')::NUMERIC + 374950) * 0.0000001175) as longitude,\n(((data -> 'location' ->> 'y')::NUMERIC + 375000) * -0.0000001172) as latitude\nfrom cache cross join jsonb_path_query(frm_data, '$[*]') as data\ncross join jsonb_path_query(data, '$.inventory[*]') as item where metric = 'storageInv'\n) as t\nwhere t.item = any(array[$item])\ngroup by \nt.building,\nt.inventory,\nt.x,\nt.y,\nt.z,\nt.longitude,\nt.latitude\n;",
